  "description": "A promise wrapped filed move / rename function",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "ts-node ./index.ts",
    "ts:defs": "tsc --declaration --outDir ./dist --emitDeclarationOnly",
